{
  "name": "@learn-react/react-router-spa",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typegen:css": "tcm src",
    "typegen:rr": "react-router typegen",
    "lint:oxc": "oxlint --type-aware",
    "lint:es": "eslint",
    "lint:style": "stylelint './**/*.css'",
    "lint": "pnpm lint:oxc && pnpm lint:es && pnpm lint:style",
    "dev:client": "react-router dev",
    "dev": "pnpm typegen:rr --watch & pnpm typegen:css -w & pnpm dev:client & pnpm typecheck --watch",
    "build": "cross-env NODE_ENV=production react-router build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@react-router/node": "7.13.0",
    "@react-router/serve": "7.13.0",
    "clsx": "2.1.1",
    "isbot": "5.1.35",
    "match-sorter": "8.2.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router": "7.13.0",
    "sort-by": "1.2.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@learn-react/linter": "workspace:*",
    "@learn-react/tsconfig": "workspace:*",
    "@react-router/dev": "7.13.0",
    "@types/node": "24.10.13",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "@types/sort-by": "1.2.3",
    "cross-env": "10.1.0",
    "eslint": "9.39.2",
    "oxlint": "1.41.0",
    "oxlint-tsgolint": "0.11.5",
    "stylelint": "17.1.1",
    "typed-css-modules": "0.9.1",
    "typescript": "5.9.3",
    "vite": "7.3.1"
  }
}
