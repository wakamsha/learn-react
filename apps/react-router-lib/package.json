{
  "name": "@learn-react/react-router-lib",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typegen:css": "tcm src",
    "lint:oxc": "oxlint --type-aware",
    "lint:es": "eslint",
    "lint:style": "stylelint './**/*.css'",
    "lint": "pnpm lint:oxc && pnpm lint:es && pnpm lint:style",
    "develop": "vite",
    "start": "pnpm typegen:css -w & pnpm typecheck --watch & pnpm develop",
    "build": "pnpm typegen:css && pnpm typecheck && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@learn-react/core": "workspace:*",
    "clsx": "2.1.1",
    "isbot": "5.1.32",
    "match-sorter": "8.2.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router": "7.9.6",
    "sort-by": "1.2.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@learn-react/builder": "workspace:*",
    "@learn-react/linter": "workspace:*",
    "@learn-react/tsconfig": "workspace:*",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/sort-by": "1.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "oxlint": "1.30.0",
    "oxlint-tsgolint": "0.8.3",
    "stylelint": "16.26.1",
    "typed-css-modules": "0.9.1",
    "typescript": "5.9.3",
    "vite": "7.2.6"
  }
