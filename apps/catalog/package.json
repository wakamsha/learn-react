{
  "name": "@learn-react/catalog",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "generate": "node ./bin/codegen.js",
    "develop": "vite",
    "type-check": "tsc --noEmit",
    "start": "run-p 'generate --watch' 'type-check --watch' 'develop {@}' --",
    "build": "pnpm generate && pnpm type-check && vite build",
    "lint:oxc": "oxlint --type-aware",
    "lint:es": "eslint",
    "lint:style": "stylelint './**/*.{ts,tsx}'",
    "lint": "pnpm lint:oxc && pnpm lint:es && pnpm lint:style",
    "capture": "rm -rf ./__screenshots__ && node ./bin/capture.js",
    "preview": "vite preview",
    "deploy": "bash ./bin/sync-s3.sh"
  },
  "dependencies": {
    "@emotion/css": "11.13.5",
    "@learn-react/core": "workspace:1.0.0",
    "@learn-react/icon": "workspace:1.0.0",
    "@learn-react/try": "workspace:1.0.0",
    "highlight.js": "11.11.1",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-router-dom": "7.13.0"
  },
  "devDependencies": {
    "@learn-react/builder": "workspace:1.0.0",
    "@learn-react/linter": "workspace:*",
    "@learn-react/tsconfig": "workspace:1.0.0",
    "@types/qs": "6.14.0",
    "@types/react": "19.2.14",
    "@types/react-dom": "19.2.3",
    "chokidar": "5.0.0",
    "eslint": "9.39.2",
    "glob": "13.0.1",
    "npm-run-all2": "8.0.4",
    "oxlint": "1.41.0",
    "oxlint-tsgolint": "0.11.5",
    "playwright": "1.58.2",
    "stylelint": "17.1.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "yargs": "18.0.0"
  }
}
