    "react-router-dom": "6.11.2"
  },
  "devDependencies": {
    "@learn-react/builder": "workspace:1.0.0",
    "@learn-react/tsconfig": "workspace:1.0.0",
    "@types/qs": "6.9.7",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "chokidar": "3.5.3",
    "glob": "10.2.2",
    "npm-run-all": "4.1.5",
