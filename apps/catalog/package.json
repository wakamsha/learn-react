  },
  "devDependencies": {
    "@learn-react/builder": "workspace:1.0.0",
    "@learn-react/tsconfig": "workspace:1.0.0",
    "@types/qs": "6.9.16",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "chokidar": "4.0.1",
