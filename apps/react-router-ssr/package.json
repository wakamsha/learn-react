{
  "name": "@learn-react/react-router-ssr",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typegen:css": "tcm src",
    "typegen:rr": "react-router typegen",
    "dev:client": "react-router dev",
    "dev": "run-p 'typegen:rr --watch' 'typegen:css -w' 'dev:client' 'typecheck --watch'",
    "build": "cross-env NODE_ENV=production react-router build",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js"
  },
  "dependencies": {
    "@react-router/fs-routes": "7.1.1",
    "@react-router/node": "7.1.1",
    "@react-router/serve": "7.1.1",
    "clsx": "2.1.1",
    "isbot": "5.1.21",
    "match-sorter": "8.0.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.1.1",
    "sort-by": "1.2.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@learn-react/tsconfig": "workspace:*",
    "@react-router/dev": "7.1.1",
    "@types/node": "22.10.5",
    "@types/react": "19.0.5",
    "@types/react-dom": "19.0.3",
    "@types/sort-by": "1.2.3",
    "cross-env": "7.0.3",
    "npm-run-all2": "7.0.2",
    "typed-css-modules": "0.9.1",
    "typescript": "5.7.3",
    "vite": "6.0.7"
  }
}
