{
  "name": "@learn-react/react-router-ssr",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "typegen:css": "tcm src",
    "typegen:rr": "react-router typegen",
    "dev:client": "react-router dev",
    "dev": "pnpm typegen:rr --watch & pnpm typegen:css -w & pnpm dev:client & pnpm typecheck --watch",
    "build": "cross-env NODE_ENV=production react-router build",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js"
  },
  "dependencies": {
    "@react-router/fs-routes": "7.8.2",
    "@react-router/node": "7.8.2",
    "@react-router/serve": "7.8.2",
    "clsx": "2.1.1",
    "isbot": "5.1.30",
    "match-sorter": "8.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-router": "7.8.2",
    "sort-by": "1.2.0",
    "tiny-invariant": "1.3.3"
  },
  "devDependencies": {
    "@learn-react/tsconfig": "workspace:*",
    "@react-router/dev": "7.8.2",
    "@types/node": "22.17.2",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@types/sort-by": "1.2.3",
    "cross-env": "7.0.3",
    "typed-css-modules": "0.9.1",
    "typescript": "5.9.2",
    "vite": "7.1.4"
  }
}
