{
  "name": "learn-react",
  "version": "1.0.0",
  "repository": "git@github.com:wakamsha/learn-react.git",
  "author": "wakamsha <wakamsha@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "engines": {
    "node": "18.12.1",
    "npm": "please_use_yarn_instead"
  },
  "packageManager": "yarn@3.2.4",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "catalog": "  yarn workspace @learn-react/catalog",
    "core": "     yarn workspace @learn-react/core",
    "doc": "      yarn workspace @learn-react/doc",
    "esnext": "   yarn workspace @learn-react/esnext",
    "icon": "     yarn workspace @learn-react/icon",
    "mobx": "     yarn workspace @learn-react/mobx",
    "routing": "  yarn workspace @learn-react/routing",
    "statement": "yarn workspace @learn-react/statement",
    "lint": "  prettier -c    packages/**/*.{ts,tsx}; eslint -c .eslintrc.cjs       packages/**/*.{ts,tsx}; stylelint packages/**/*.{ts,tsx}",
    "format": "prettier -c -w packages/**/*.{ts,tsx}; eslint -c .eslintrc.cjs --fix packages/**/*.{ts,tsx}; stylelint --fix packages/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@typescript-eslint/eslint-plugin": "5.42.1",
    "@typescript-eslint/parser": "5.42.1",
    "@vitejs/plugin-react": "2.2.0",
    "cheerio": "1.0.0-rc.12",
    "chokidar": "3.5.3",
    "cspell": "6.13.3",
    "eslint": "8.26.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.1.4",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "8.0.3",
    "husky": "8.0.2",
    "jest": "29.2.2",
    "jest-environment-jsdom": "29.2.2",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "postcss-syntax": "0.36.2",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.1.1",
    "stylelint": "14.14.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-recess-order": "3.0.0",
    "stylelint-config-recommended": "9.0.0",
    "svgo": "3.0.0",
    "ts-jest": "29.0.3",
    "ts-xor": "1.0.8",
    "typedoc": "0.23.20",
    "typedoc-plugin-missing-exports": "1.0.0",
    "typedoc-theme-hierarchy": "3.0.0",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "yargs": "17.6.2"
  },
  "dependencies": {
    "@emotion/css": "11.10.5",
    "constate": "3.3.2",
    "csx": "10.0.2",
    "date-fns": "2.29.3",
    "highlight.js": "11.6.0",
    "hotkeys-js": "3.10.0",
    "mobx": "6.6.2",
    "mobx-react": "7.5.3",
    "pdfjs-dist": "3.0.279",
    "qs": "6.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.4.3",
    "swr": "1.3.0"
  },
  "lint-staged": {
    "packages/**/src/**/*.{ts,tsx}": [
      "cspell",
      "prettier -c -w",
      "eslint -c .eslintrc.cjs --fix",
      "stylelint --cache --fix"
    ]
  }
}
