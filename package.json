{
  "name": "learn-react",
  "version": "1.0.0",
  "repository": "git@github.com:wakamsha/learn-react.git",
  "author": "wakamsha <wakamsha@gmail.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": "18.15.0",
    "pnpm": "7.29.3",
    "npm": "please use pnpm instead"
  },
  "packageManager": "pnpm@7.29.3",
  "scripts": {
    "postinstall": "husky install",
    "catalog": "  pnpm -F \"@learn-react/catalog\"",
    "core": "     pnpm -F \"@learn-react/core\"",
    "doc": "      pnpm -F \"@learn-react/doc\"",
    "esnext": "   pnpm -F \"@learn-react/esnext\"",
    "icon": "     pnpm -F \"@learn-react/icon\"",
    "routing": "  pnpm -F \"@learn-react/routing\"",
    "statement": "pnpm -F \"@learn-react/statement\"",
    "lint": "  prettier -c    packages/**/*.{ts,tsx}; eslint -c .eslintrc.cjs packages/**/*.{ts,tsx};       stylelint packages/**/*.{ts,tsx}",
    "format": "prettier -c -w packages/**/*.{ts,tsx}; eslint -c .eslintrc.cjs packages/**/*.{ts,tsx} --fix; stylelint packages/**/*.{ts,tsx} --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@types/jest": "29.4.4",
    "@types/node": "18.15.5",
    "@types/qs": "6.9.7",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.56.0",
    "@typescript-eslint/parser": "5.56.0",
    "@vitejs/plugin-react": "3.1.0",
    "cheerio": "1.0.0-rc.12",
    "chokidar": "3.5.3",
    "cspell": "6.29.3",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "glob": "9.3.2",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.21",
    "postcss-syntax": "0.36.2",
    "prettier": "2.8.6",
    "prettier-plugin-organize-imports": "3.2.2",
    "stylelint": "15.2.0",
    "stylelint-config-recess-order": "4.0.0",
    "stylelint-config-recommended": "10.0.1",
    "stylelint-config-standard": "30.0.1",
    "svgo": "3.0.2",
    "ts-jest": "29.0.5",
    "ts-xor": "1.1.0",
    "typedoc": "0.23.28",
    "typedoc-plugin-missing-exports": "1.0.0",
    "typedoc-theme-hierarchy": "3.1.0",
    "typescript": "4.9.5",
    "vite": "4.2.1",
    "yargs": "17.7.1"
  },
  "dependencies": {
    "@emotion/css": "11.10.6",
    "constate": "3.3.2",
    "csx": "10.0.2",
    "date-fns": "2.29.3",
    "highlight.js": "11.7.0",
    "hotkeys-js": "3.10.1",
    "mobx": "6.8.0",
    "mobx-react": "7.6.0",
    "pdfjs-dist": "3.4.120",
    "qs": "6.11.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.9.0",
    "swr": "2.1.1"
  },
  "lint-staged": {
    "packages/**/src/**/*.{ts,tsx}": [
      "cspell",
      "prettier -c",
      "eslint -c .eslintrc.cjs",
      "stylelint --cache"
    ]
  }
