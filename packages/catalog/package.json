{
  "name": "@learn-react/catalog",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "dependencies": {
    "@learn-react/core": "1.0.0"
  },
  "scripts": {
    "reset": "rm -rf dist/; mkdir -p dist/;",
    "copy": "cp -r ./public/. ./dist/",
    "generate": "node ./bin/codegen",
    "develop": "webpack serve",
    "type-check": "tsc --noEmit",
    "start": "yarn reset; yarn copy; run-p \"develop\" \"type-check --watch\" \"generate --watch\"",
    "build": "yarn reset; yarn copy; yarn generate; webpack --mode production"
  }
}
