{
  "name": "@learn-react/routing",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "keywords": [
    "react-router-dom"
  ],
  "scripts": {
    "reset": "rm -rf dist/; mkdir -p dist/assets/;",
    "copy": "cp src/index.html dist/index.html;",
    "script": "webpack --progress --colors --config ../../webpack.config.js",
    "serve": "browser-sync start -c ../../bs-config.js",
    "start": "yarn reset; yarn copy; run-p \"script -- -d -w --develop {1}\" \"serve -- {2}\" --",
    "build": "yarn reset; yarn copy; yarn script"
  }
}
