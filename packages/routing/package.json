{
  "name": "@learn-react/routing",
  "version": "1.0.0",
  "license": "UNLICENSED",
  "keywords": [
    "react-router-dom"
  ],
  "dependencies": {
    "@learn-react/core": "1.0.0"
  },
  "scripts": {
    "reset": "rm -rf dist/; mkdir -p dist/assets/;",
    "copy": "cp -r ./public/. ./dist/",
    "develop": "webpack serve",
    "type-check": "tsc --noEmit",
    "start": "yarn reset; yarn copy; run-p \"develop\" \"type-check --watch\"",
    "build": "yarn reset; yarn copy; webpack --mode production"
  }
}
